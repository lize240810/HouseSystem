    <![endif]-->
    <style type="text/css">
        ul.nav:nth-child(2){
          float: right;
        }
    </style>
  </head>

  <body>
    {% include 'includes/navbar.htm.j2' ignore missing with context %}

    <div class="container">

      <div class="row row-offcanvas row-offcanvas-right">

        <div class="col-xs-12 col-sm-9">
          <p class="pull-right visible-xs">
            <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas">展开标签</button>
          </p>
            {# 头部显示栏 #}
            {% block content_header %}
              {% include 'includes/content_header.htm.j2' ignore missing with context %}
            {% endblock %}

            {# 内容原始页 #}
            {% block content_row %}
              {% include 'includes/content_row.htm.j2' ignore missing with context %}
            {% endblock %}
        </div><!--/.col-xs-12.col-sm-9-->

        {% include 'includes/content_nav.htm.j2' ignore missing with context %}
      </div><!--/row-->

      <hr>

      <footer>
        <p>© Company 2014</p>
      </footer>

    </div><!--/.container-->


    <!-- Bootstrap core JavaScript
    ================================================== -->
